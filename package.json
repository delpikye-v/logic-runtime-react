{
  "name": "logic-runtime-react-z",
  "version": "2.0.2",
  "description": "Intent-first runtime for React. No hooks. Deterministic state. Orchestrated effects.",
  "license": "MIT",
  "author": "Delpi.Kye",

  "sideEffects": false,

  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",

  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.esm.js",
      "require": "./build/index.cjs.js"
    },
    "./react": {
      "types": "./build/react/index.d.ts",
      "import": "./build/react/index.esm.js",
      "require": "./build/react/index.cjs.js"
    },
    "./devtools": {
      "types": "./build/devtools/index.d.ts",
      "import": "./build/devtools/index.esm.js",
      "require": "./build/devtools/index.cjs.js"
    }
  },

  "files": ["build"],

  "scripts": {
    "clean": "rimraf build",
    "build": "rollup -c",
    "cb": "npm run clean && npm run build",
    "prepublishOnly": "npm run cb"
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/delpikye-v/logic-runtime-react.git"
  },

  "homepage": "https://github.com/delpikye-v/logic-runtime-react#readme",
  "bugs": {
    "url": "https://github.com/delpikye-v/logic-runtime-react/issues"
  },

  "keywords": [
    "react",
    "react-no-hook",
    "external-store",
    "intent-runtime",
    "logic-runtime",
    "computed-state",
    "no-hooks",
    "react-architecture",
    "external-runtime"
  ],

  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },

  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.3.3",
    "tslib": "^2.6.2",

    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17"
  }
}
